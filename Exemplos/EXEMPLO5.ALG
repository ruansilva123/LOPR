Algoritmo "Exemplo5"

Var
   mediaAluno: real
   frequenciaAluno: real
Inicio
      escreval("Qual sua média?")
      leia(mediaAluno)
      escreval("Qual sua frequência?")
      leia(ferquenciaAluno)
      
      SE (mediaAluno >= 7.0)E(frequenciaAluno >= 75.0) ENTAO
         escreval("Aprovado!")
      SENAO
         escreval("Reprovado!")
      FIMSE
Fimalgoritmo