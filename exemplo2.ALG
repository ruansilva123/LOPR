Algoritmo "calculadora"

Var
   //variaveis
   numeroA: inteiro
   numeroB: inteiro
   resultado: real
Inicio
   //soma
   escreval("Escolha um número: ")
   leia(numeroA)
   escreval("Escolha outro número: ")
   leia(numeroB)
   resultado <- numeroA + numeroB
   escreva ("O resultado é: " , resultado)
Fimalgoritmo